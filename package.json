{
  "name": "typezen",
  "description": "A simple type checker for TypeScript",
  "version": "0.0.4",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "license": "MIT",
  "homepage": "https://typezen.vercel.app",
  "repository": {
    "url": "https://github.com/kiron0/typezen"
  },
  "author": "Toufiq Hasan Kiron (https://toufiqhasankiron.com)",
  "scripts": {
    "build": "tsup",
    "release": "npm run build && changeset publish",
    "lint": "tsc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@types/node": "^22.13.1",
    "rollup": "^4.34.6",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "typechecker",
    "check-type",
    "javascript-type",
    "primitive-types",
    "plain-object",
    "plain-objects",
    "class-instance",
    "class-identifier",
    "type-checking",
    "type-checker",
    "type-check",
    "define-type",
    "get-type",
    "what-type",
    "is-object",
    "is-plain-obj",
    "is-plain-object"
  ]
}
